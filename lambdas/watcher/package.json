{
  "name": "race-results-watcher",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "esbuild index.ts --bundle --external:@aws-sdk --sourcemap --platform=node --target=es2020 --metafile=dist/metadata.json --outfile=dist/index.js",
    "postbuild": "cd dist && zip -r index.zip index.js*",
    "build-visualizer": "esbuild-visualizer --metadata dist/metadata.json --filename dist/stats.html --open",
    "local": "node --experimental-strip-types run-local.ts",
    "deploy": "aws lambda update-function-code --function-name race-results-watcher-stage --output json --no-cli-pager --zip-file fileb://dist/index.zip && aws lambda update-function-code --function-name race-results-watcher-prod --output json --no-cli-pager --zip-file fileb://dist/index.zip"
  },
  "author": "Alex Clement",
  "license": "ISC",
  "description": "",
  "type": "module",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.149",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.15.26",
    "@types/short-hash": "^1.0.3",
    "esbuild": "^0.25.5",
    "esbuild-visualizer": "^0.7.0"
  },
  "dependencies": {
    "cheerio": "^1.0.0",
    "csv": "^6.3.11",
    "deep-object-diff": "^1.1.9",
    "fetch-cookie": "^3.1.0",
    "jest-diff": "^30.0.0-beta.3",
    "lodash-es": "^4.17.21",
    "short-hash": "^1.0.0"
  }
}
